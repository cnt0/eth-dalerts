{
  "name": "donationalerts",
  "version": "0.0.1",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": 65,
            "firefox": 59
          }
        }
      ],
      "react"
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "semi": false
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "web3-promisify": "^1.0.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-serve": "^0.2.0"
  },
  "dependencies": {
    "zeppelin-solidity": "^1.9.0"
  }
}
